# Complete the abbreviation function below.
def abbreviation(a, b):
    if(set(a.lower())==set(b.lower())):
        return("YES")
    else: return("NO")
if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    q = int(input())

    for q_itr in range(q):
        a = input()

        b = input()

        result = abbreviation(a, b)

        fptr.write(result + '\n')

    fptr.close()
